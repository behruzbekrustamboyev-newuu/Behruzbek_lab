#include <iostream>
using namespace std;

int main() {
    char choice;
    cout << "Choose language : ";
    cin >> choice;

    switch (choice) {
        case 'u': cout << "Salom!" << endl; break;
        case 'e': cout << "Hello!" << endl; break;
        case 'r': cout << "Privet!" << endl; break;
        case 'g': cout << "Hallo!" << endl; break;
        default: cout << "error" << endl; break;
    }

    return 0;
}
